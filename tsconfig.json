{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "module": "commonjs",
    "paths": {
      "_src/*": [
        "./src/*"
      ]
    },
    "sourceMap": true,
    "target": "es5"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "public/**/*"
  ],
  "compileOnSave": false
}