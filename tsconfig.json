{
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react",
    "module": "commonjs",
    "paths": {
      "_src/*": [
        "src/*"
      ]
    },
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "dist", "public"
  ],
  "include": [
    "./src"
  ],
  "compileOnSave": false
}